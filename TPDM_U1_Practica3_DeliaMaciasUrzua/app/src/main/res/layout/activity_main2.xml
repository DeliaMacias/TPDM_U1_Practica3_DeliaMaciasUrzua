<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:padding="10dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/font"
    tools:context=".Main2Activity">

    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="CARTOON NETWORK"
        android:textStyle="bold"
        android:textColor="#000000"
        android:textAlignment="center"
        android:textSize="29dp"
        android:id="@+id/titulo"
        />
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:text="Chapas las supernenas solo por el canal 5!"
        android:textAlignment="center"
        android:layout_marginTop="34dp"
        android:textSize="22dp"
        android:id="@+id/subtitulo"
        />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="120dp"
        android:src="@drawable/act23"
        android:layout_marginTop="95dp"
        android:id="@+id/imgcenter"
        />
    <ImageView
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:src="@drawable/act24"
        android:layout_marginTop="175dp"
        android:layout_marginRight="20dp"
        />
    <ImageView
        android:layout_width="90dp"
        android:layout_height="100dp"
        android:src="@drawable/act26"
        android:layout_alignRight="@id/imgcenter"
        android:layout_marginTop="175dp"
        android:layout_marginLeft="20dp"
        />
    <ImageView
        android:layout_width="match_parent"
        android:layout_height="110dp"
        android:src="@drawable/act25"
        android:layout_below="@id/imgcenter"
        android:layout_marginTop="20dp"
        android:id="@+id/imgbelow"
        />
    <Button
        android:layout_width="match_parent"
        android:layout_height="40dp"
        android:background="#FFAA00"
        android:text="VER SERIES"
        android:textSize="20dp"
        android:layout_below="@id/imgbelow"
        android:layout_marginTop="25dp"
        android:id="@+id/btncenter"
        />
    <Button
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:text="online"
        android:layout_below="@id/btncenter"
        android:layout_marginTop="10dp"
        android:id="@+id/btnleft"

        />
    <Button
        android:layout_width="180dp"
        android:layout_height="40dp"
        android:text="online"
        android:layout_toRightOf="@id/btnleft"
        android:layout_below="@id/btncenter"
        android:layout_marginTop="10dp"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:src="@drawable/ajustes"
        android:layout_below="@id/btnleft"
        android:layout_marginTop="5dp"
        android:layout_marginLeft="24dp"
        android:id="@+id/ico1"
        />
    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:src="@drawable/basura"
        android:layout_below="@id/btnleft"
        android:layout_toRightOf="@id/ico1"
        android:layout_marginTop="5dp"
        android:id="@+id/ico2"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:src="@drawable/bloqueado"
        android:layout_below="@id/btnleft"
        android:layout_toRightOf="@id/ico2"
        android:layout_marginTop="5dp"
        android:id="@+id/ico3"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:src="@drawable/editar"
        android:layout_below="@id/btnleft"
        android:layout_toRightOf="@id/ico3"
        android:layout_marginTop="5dp"
        android:id="@+id/ico4"
        />

    <ImageView
        android:layout_width="wrap_content"
        android:layout_height="match_parent"
        android:src="@drawable/ajustes"
        android:layout_below="@id/btnleft"
        android:layout_toRightOf="@id/ico4"
        android:layout_marginTop="5dp"
        android:id="@+id/ico5"
        />

</RelativeLayout>